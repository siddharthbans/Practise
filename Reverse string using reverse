import java.util.Scanner;

public class Reurs {
    public static void Reversestring(String n, int i) {
        if (i==1){
            System.out.println(n.charAt(i));
            return;
        }
        System.out.println(n.charAt(i));
        Reversestring(n, i-1);
        
    }
    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);
        String n = sc.next();
        int i = n.length() -1;
        Reversestring(n, i);

    }}
